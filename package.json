{
  "name": "eslint-plugin-class-extends",
  "version": "1.0.0",
  "description": "ESLint rule to prevent extending classes.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Wes Baker",
  "main": "lib/index.js",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint ./",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "istanbul cover node_modules/mocha/bin/_mocha tests/**/*.js",
    "watch": "mocha tests --recursive --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wesbaker/eslint-plugin-class-extends"
  },
  "homepage": "https://github.com/wesbaker/eslint-plugin-class-extends",
  "bugs": "https://github.com/wesbaker/eslint-plugin-class-extends/issues",
  "dependencies": {
    "requireindex": "~1.2.0"
  },
  "devDependencies": {
    "coveralls": "3.1.0",
    "eslint": "7.7.0",
    "istanbul": "0.4.5",
    "mocha": "8.1.3"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "renovate": {
    "extends": [
      "github>wesbaker/renovate-config"
    ]
  }
}
